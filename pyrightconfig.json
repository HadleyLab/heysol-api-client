{
    "include": [
        "src",
        "tests"
    ],
    "exclude": [
        "**/__pycache__",
        "**/.git",
        "**/htmlcov",
        "**/.pytest_cache",
        "**/.benchmarks",
        "**/.ruff_cache"
    ],
    "pythonVersion": "3.8",
    "typeCheckingMode": "basic",
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": "none",
    "pythonPath": "./src"
}